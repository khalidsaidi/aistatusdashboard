# Firebase Test Configuration Example
# Copy this file to config/test.env and fill in your actual values
# These credentials are for the ai-status-dashboard-dev project

FIREBASE_PROJECT_ID=ai-status-dashboard-dev
FIREBASE_API_KEY=your-api-key-here
FIREBASE_AUTH_DOMAIN=ai-status-dashboard-dev.firebaseapp.com
FIREBASE_STORAGE_BUCKET=ai-status-dashboard-dev.firebasestorage.app
FIREBASE_MESSAGING_SENDER_ID=your-sender-id-here
FIREBASE_APP_ID=your-app-id-here

# Test Configuration
NODE_ENV=test
JEST_TIMEOUT=60000

# Worker Queue Configuration for Testing
WORKER_CONCURRENCY=2
WORKER_MAX_RETRIES=2
WORKER_BACKOFF_DELAY=1000
WORKER_STALLED_INTERVAL=5000
WORKER_MAX_STALLED_COUNT=1
WORKER_MAX_QUEUE_SIZE=1000
WORKER_RATE_LIMIT=50
CIRCUIT_BREAKER_THRESHOLD=5
CIRCUIT_BREAKER_TIMEOUT=30000 