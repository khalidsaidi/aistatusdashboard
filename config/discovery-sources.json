{
  "sources": [
    {
      "name": "Artificial Analysis",
      "url": "https://artificialanalysis.ai/models",
      "type": "html",
      "enabled": true,
      "patterns": [
        "(?:api\\.)?([a-z0-9-]+)\\.(?:ai|com|org)(?:\\/api)?",
        "([a-z0-9-]+)\\s+(?:ai|llm|language model|api)"
      ],
      "confidence": 0.8,
      "description": "Comprehensive AI model analysis platform"
    },
    {
      "name": "Hugging Face Models",
      "url": "https://huggingface.co/models",
      "type": "html", 
      "enabled": true,
      "patterns": [
        "\"([a-z0-9-]+)\\/[^\"]*\"",
        "([a-z0-9-]+)\\s+(?:model|api|inference)"
      ],
      "confidence": 0.9,
      "description": "Leading ML model repository"
    },
    {
      "name": "GitHub AI Awesome Lists",
      "url": "https://api.github.com/search/repositories?q=awesome+ai+llm+language+model&sort=updated&per_page=10",
      "type": "github",
      "enabled": true,
      "patterns": [
        "(?:https?:\\/\\/)?(?:api\\.)?([a-z0-9-]+)\\.(?:ai|com|org)(?:\\/api)?",
        "([a-z0-9-]+)\\s+(?:ai|llm|language model|api)",
        "\\*\\s*\\[([a-z0-9-]+)\\]"
      ],
      "confidence": 0.7,
      "description": "Community-curated awesome lists"
    },
    {
      "name": "Product Hunt AI",
      "url": "https://www.producthunt.com/topics/artificial-intelligence",
      "type": "html",
      "enabled": true,
      "patterns": [
        "([a-z0-9-]+)\\.ai",
        "([a-z0-9-]+)\\s+AI",
        "data-name=\"([a-z0-9-]+)\""
      ],
      "confidence": 0.6,
      "description": "Latest AI product launches"
    },
    {
      "name": "AI News Reddit",
      "url": "https://www.reddit.com/r/artificial/hot.json?limit=25",
      "type": "reddit",
      "enabled": true,
      "patterns": [
        "([a-z0-9-]+)\\.ai",
        "([a-z0-9-]+)\\s+(?:AI|API|LLM)",
        "\\b([a-z0-9-]+)\\s+(?:launches|releases|announces)"
      ],
      "confidence": 0.5,
      "description": "AI community discussions and news"
    },
    {
      "name": "Papers With Code",
      "url": "https://paperswithcode.com/sota",
      "type": "html",
      "enabled": true,
      "patterns": [
        "([a-z0-9-]+)\\s+(?:model|api|inference)",
        "by\\s+([a-z0-9-]+)",
        "\"([a-z0-9-]+)\""
      ],
      "confidence": 0.7,
      "description": "State-of-the-art ML models"
    }
  ],
  "knownProviders": [
    "openai",
    "anthropic", 
    "google-ai",
    "huggingface",
    "cohere",
    "replicate", 
    "groq",
    "deepseek",
    "meta",
    "xai",
    "perplexity",
    "claude",
    "mistral",
    "aws",
    "azure",
    "together",
    "fireworks",
    "anyscale",
    "runpod",
    "modal"
  ],
  "filters": {
    "minLength": 3,
    "maxLength": 20,
    "excludeWords": [
      "api", "www", "http", "https", "com", "org", "net", "github", 
      "model", "chat", "gpt", "bot", "test", "demo", "example",
      "admin", "user", "login", "auth", "docs", "help", "support"
    ],
    "requiredConfidence": 0.5
  },
  "notification": {
    "email": {
      "enabled": true,
      "template": "detailed"
    },
    "github": {
      "enabled": true,
      "createIssue": true,
      "labels": ["ai-discovery", "automated", "new-provider"]
    },
    "slack": {
      "enabled": false,
      "webhook": ""
    }
  },
  "schedule": {
    "frequency": "daily",
    "time": "09:00",
    "timezone": "UTC"
  },
  "storage": {
    "keepHistoryDays": 30,
    "maxResultsPerRun": 50
  }
} 